{
    "name": "SN Search",
    "description": "Enables Google Chrome omnibox to search Service-Now tickets by entering \"sn\" and ticket number.",
    "version": "1.0.2",
    "manifest_version": 2,
    "omnibox": {
      "keyword":"sn"  
    },
    "icons": { 
        "128": "icon_128.png", 
        "48": "icon_48.png", 
        "16": "icon_16.png" 
    },
    "background": { 
        "scripts":["background.js"]
    },
    "minimum_chrome_version":"9",
//    "options_page":"options.html",
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },  
    "permissions":[ "storage" ]
}